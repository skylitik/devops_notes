# collect artifacts (can disable)
---
- name: Understanding vars
  hosts: all
  become: yes
  gather_facts: False
  vars:
    USRNM: playuser
    COMM: variable from playbook
  tasks:
    - name: create user
      ansible.builtin.user:
        name: "{{USRNM}}"
        comment: "{{COMM}}"
      register: usrout
    - debug:
        var: usrout.name

    - debug:
        var: usrout.comment
# Now can execute comand 
ansible -m setup web01